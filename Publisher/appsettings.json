{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Aspire": {
    "RabbitMQ": {
      "Client": {
        "ConnectionString": "amqp://guest:guest@localhost:5672",
        "DisableHealthChecks": false,
        "DisableTracing": false,
        "MaxConnectRetryCount": 2
      }
    }
  }
}
